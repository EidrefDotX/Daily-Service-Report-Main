{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" },
    { "src": "manifest.json", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/login.html" },
    { "src": "^/auth/(.*)", "dest": "/api/index.py" },
    { "src": "^/reports$", "dest": "/api/index.py" },
    ... (API routes)
  ]
}
